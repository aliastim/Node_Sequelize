<h1>Liste des utilisateurs :</h1>

 <table class="table">
   <thead class="thead-dark">
     <tr>
       <th scope="col">id</th>
       <th scope="col">Prénom</th>
       <th scope="col">Nom</th>
       <th scope="col">Email</th>
       <th scope="col">Créé le</th>
       <th scope="col">Mis à jour le</th>
     </tr>
   </thead>
      <tbody>
{{#each users}}
     <tr>
       <th scope="row">{{id}}</th>
       <td>{{firstname}}</td>
       <td>{{name}}</td>
       <td>{{email}}</td>
       <td>{{createdAt}}</td>
       <td>{{updatedAt}}</td>
     </tr>

{{else}}
<p>Aucun utilisateur</p>

{{/each}}

   </tbody>
 </table>